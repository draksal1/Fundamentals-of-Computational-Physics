   "source": [
    "def calculate_machine_epsilon():\n",
    "    epsilon = 1.0\n",
    "    while (1.0 + epsilon / 2.0 != 1.0):\n",
    "        epsilon /= 2.0\n",
    "    return epsilon"
   ]
  },
